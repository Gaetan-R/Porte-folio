{% extends 'base.html.twig' %}

{% block html %}

{% block title %}Liste des projets{% endblock %}

{% block body %}


    <section>

    {% for projets in projets %}

        <div class="container">
            <div class="content">
                <div class="grid">
                    <figure class="effect-bubba">
                        <img src="{{projets.poster}}" alt="{{ projets.nom }} poster">
                        <figcaption>
                            <h2>Projet <span>{{ projets.nom }}</span></h2>
                            <p>{{ projets.description }}</p>
                            <a class="buy" href="{{ path('projet_show', {id : projets.id }) }}">Voir</a>
                        </figcaption>
                    </figure>
                </div>
            </div>
        </div>

    {% endfor %}
    </section>



{% endblock %}
{% endblock %}

